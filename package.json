{
  "name": "robolist",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "studio": "cd packages/prisma-schema && npx prisma studio",
    "ci": "cd apps/web && npm run ci"
  },
  "devDependencies": {
    "eslint-config-custom": "*",
    "prettier": "2.7.1",
    "turbo": "latest"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@mantine/core": "^4.2.12",
    "@mantine/dates": "^4.2.12",
    "@mantine/form": "^4.2.12",
    "@mantine/hooks": "^4.2.12",
    "@mantine/modals": "^4.2.12",
    "@mantine/next": "^4.2.12",
    "@mantine/notifications": "^4.2.12",
    "babel-plugin-superjson-next": "^0.4.3",
    "dayjs": "^1.11.3",
    "superjson": "^1.9.1"
  },
  "packageManager": "npm@8.12.1"
}
